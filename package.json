{
  "name": "jaeles-integration",
  "version": "1.0.8",
  "description": "Library to communicate with Jaeles server for fuzzing http servers",
  "main": "dist/index.js",
  "scripts": {
    "build:dev": "tsc --watch",
    "test": "ts-mocha ./test/**/*_test.ts",
    "build": "npm i && rm -rf dist && tsc && rm -rf node_modules && npm i --production",
    "lint": "eslint ./src/**/*.ts",
    "own:publish": "npm run build && npm publish"
  },
  "keywords": [
    "integration",
    "infosec",
    "security-tools",
    "jaeles",
    "burp"
  ],
  "author": "drstrain",
  "license": "MIT",
  "devDependencies": {
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.35",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "chai": "^4.3.6",
    "eslint": "^8.16.0",
    "eslint-config-google": "^0.14.0",
    "ts-mocha": "^10.0.0",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "axios-burp": "^1.0.14"
  }
}